{
	"name": "pl_loadbanksdata",
	"properties": {
		"activities": [
			{
				"name": "Copy data from Bankfile",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "SqlServerSink",
						"preCopyScript": "truncate table [stage].[BanksData]",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "Date",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Date",
									"type": "DateTime",
									"physicalType": "date"
								}
							},
							{
								"source": {
									"name": "Amount",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Amount",
									"type": "Int32",
									"physicalType": "int"
								}
							},
							{
								"source": {
									"name": "AcctType",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "AcctType",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "OpenedBy",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "OpenedBy",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "Branch",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Branch",
									"type": "String",
									"physicalType": "varchar"
								}
							},
							{
								"source": {
									"name": "Customer",
									"type": "String",
									"physicalType": "String"
								},
								"sink": {
									"name": "Customer",
									"type": "String",
									"physicalType": "varchar"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_Banks_src",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_sql_sink",
						"type": "DatasetReference",
						"parameters": {
							"schema": "stage",
							"table": "BanksData"
						}
					}
				]
			},
			{
				"name": "ArchiveBankFile",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Copy data from Bankfile",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_archive_src",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ds_archive_sink",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Delete Source file",
				"type": "Delete",
				"dependsOn": [
					{
						"activity": "ArchiveBankFile",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "ds_Banks_src",
						"type": "DatasetReference"
					},
					"enableLogging": false,
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"recursive": true,
						"enablePartitionDiscovery": false
					}
				}
			}
		],
		"annotations": []
	}
}